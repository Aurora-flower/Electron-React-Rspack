    "@eslint/js": "9.26.0",
    "@eslint/json": "0.12.0",
    "@eslint/markdown": "6.4.0",
    "@rspack/core": "1.3.10",
    "@rspack/plugin-react-refresh": "1.4.3",
    "@swc/core": "1.11.24",
