    "test": "cross-env NODE_ENV=development gulp rs --name=rs",
    "start": "electron .",
    "i:legacy": "npm install --legacy-peer-deps",
    "up": "npm update --major",
    "i": "npm install --registry=https://registry.npmjs.org/",
    "preinstall": "echo \"running install...\"",
    "check:out": "node build/scripts/update-deps.js",
    "clean:npm": "npm cache clean --force",
    "mac:chown": "sudo chown -R $USER ~/.npm",
    "eslint:inspect": "eslint --inspect-config",
    "package": "rimraf release && electron-builder",
    "dev": "cross-env NODE_ENV=development gulp dev",
    "gulp:package": "gulp package",
    "compile": "gulp compile",
    "package:dir": "rimraf release && electron-builder --dir",
    "check:audit": "npm audit --registry=https://registry.npmjs.org/",
    "fix:audit": "npm audit fix --registry=https://registry.npmjs.org/",
    "lint": "eslint . --ignore-pattern 'gen/*' --ignore-pattern 'test/*'",
    "tailwind": "npx @tailwindcss/cli -i source/src/static/stylesheets/tailwind.css -o source/src/static/stylesheets/tailwind_output.css",
    "prepare": "husky"
  },
