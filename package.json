{
  "name": "electron-react-rspack",
  "version": "0.0.3",
  "productName": "HuaYing",
  "description": "花楹一间，向往自由的发现之旅。",
  "main": "app/electron",
  "private": "true",
  "scripts": {
    "prepare": "husky",
    "preinstall": "patch-package",
    "start": "electron .",
    "compile": "gulp",
    "clean:cache": "rimraf app release/win-unpacked",
    "build": "npm run clean:cache && run-p \"package:prepare\" \"compile\"",
    "dev": "gulp dev",
    "lint:debug": "eslint --debug",
    "lint:fix": "eslint --fix",
    "lint:info": "eslint --quiet",
    "lint": "biome check --write",
    "package:prepare": "ts-node ./scripts/build/package-prepare.ts",
    "package": "npm run build && electron-builder --publish always"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Aurora-flower/Electron-React-Rspack.git"
  },
  "keywords": ["electron", "react", "rspack", "gulp", "pixi", "three"],
  "author": "Aurora-flower",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Aurora-flower/Electron-React-Rspack/issues"
  },
  "homepage": "https://github.com/Aurora-flower/Electron-React-Rspack#readme",
  "devDependencies": {
    "@babel/core": "7.26.10",
    "@babel/plugin-syntax-jsx": "7.25.9",
    "@babel/preset-env": "7.26.9",
    "@babel/preset-react": "7.26.3",
    "@babel/preset-typescript": "7.27.0",
    "@biomejs/biome": "1.9.4",
    "@commitlint/cli": "19.8.0",
    "@commitlint/config-conventional": "19.8.0",
    "@eslint/css": "0.7.0",
    "@eslint/js": "9.25.1",
    "@eslint/json": "0.12.0",
    "@eslint/markdown": "6.4.0",
    "@rspack/core": "1.3.8",
    "@rspack/plugin-react-refresh": "1.4.1",
    "@swc/core": "1.11.24",
    "@swc/helpers": "0.5.17",
    "@tailwindcss/postcss": "4.1.4",
    "@tsconfig/node22": "22.0.1",
    "@types/css-modules": "1.0.5",
    "@types/gulp": "4.0.17",
    "@types/node": "22.15.3",
    "@types/react": "19.1.2",
    "@types/react-dom": "19.1.2",
    "@typescript-eslint/parser": "8.31.1",
    "babel-loader": "10.0.0",
    "babel-plugin-react-compiler": "19.1.0-rc.1",
    "css-loader": "7.1.2",
    "electron": "36.0.0",
    "electron-builder": "26.0.12",
    "eslint": "9.25.1",
    "eslint-plugin-react": "7.37.5",
    "file-loader": "6.2.0",
    "globals": "16.0.0",
    "gulp": "5.0.0",
    "husky": "9.1.7",
    "npm-run-all2": "7.0.2",
    "patch-package": "8.0.0",
    "postcss-loader": "8.1.1",
    "react-refresh": "0.17.0",
    "rimraf": "6.0.1",
    "tailwindcss": "4.1.4",
    "ts-loader": "9.5.2",
    "ts-node": "10.9.2",
    "typescript-eslint": "8.31.1"
  },
  "dependencies": {
    "electron-log": "5.3.4",
    "electron-updater": "6.6.2",
    "express": "5.1.0",
