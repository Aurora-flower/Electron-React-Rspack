{
  "name": "electron-react-rspack",
  "version": "0.0.4",
  "productName": "HuaYing",
  "description": "花楹一间，向往自由的发现之旅。",
  "main": "app/electron",
  "private": "true",
  "scripts": {
    "i": "npm install --legacy-peer-deps && npm outdated",
    "prepare": "husky",
    "preinstall": "patch-package",
    "start": "electron .",
    "check:updates": "npm-check-updates -u",
    "compile": "gulp",
    "clean:cache": "rimraf app release/win-unpacked",
    "debug:dev": "cross-env NODE_ENV=development rspack dev",
    "debug:build": "cross-env NODE_ENV=development rspack build",
    "build": "npm run clean:cache && run-p \"package:prepare\" \"compile\"",
    "dev": "cross-env NODE_ENV=development gulp dev",
    "lint:debug": "eslint --debug",
    "lint:fix": "eslint --fix",
    "lint:info": "eslint --quiet",
    "format": "prettier --write .",
    "biome:lint": "biome check --write",
    "package:prepare": "cross-env NODE_ENV=production ts-node ./scripts/build/package-prepare.ts",
    "package": "npm run build && electron-builder --publish always"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Aurora-flower/Electron-React-Rspack.git"
  },
  "keywords": [
    "electron",
    "react",
    "rspack",
    "gulp",
    "pixi",
    "three",
    "typescript",
    "webGL",
    "primereact",
    "pixi-ui"
  ],
  "author": "Aurora-flower",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Aurora-flower/Electron-React-Rspack/issues"
  },
